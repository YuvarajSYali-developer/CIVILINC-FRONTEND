{"ast":null,"code":"import DangerousHTML from 'dangerous-html/vue';\nimport Chart from 'chart.js';\nexport default {\n  name: 'Page03',\n  props: {},\n  components: {\n    DangerousHTML: DangerousHTML\n  },\n  data: function data() {\n    return {\n      rawv1yq: ' ',\n      chart: null,\n      chartData: {\n        labels: ['Roads', 'Water', 'Electricity', 'Sanitation', 'Other'],\n        datasets: [{\n          label: 'Complaints by Category',\n          data: [45, 30, 25, 20, 15],\n          backgroundColor: ['rgba(0, 255, 135, 0.8)', 'rgba(96, 239, 255, 0.8)', 'rgba(255, 99, 132, 0.8)', 'rgba(54, 162, 235, 0.8)', 'rgba(255, 206, 86, 0.8)'],\n          borderColor: ['rgba(0, 255, 135, 1)', 'rgba(96, 239, 255, 1)', 'rgba(255, 99, 132, 1)', 'rgba(54, 162, 235, 1)', 'rgba(255, 206, 86, 1)'],\n          borderWidth: 1\n        }]\n      }\n    };\n  },\n  metaInfo: {\n    title: 'Dashboard - Civilinc',\n    meta: [{\n      property: 'og:title',\n      content: 'Dashboard - Civilinc'\n    }]\n  },\n  mounted: function mounted() {\n    var _this = this;\n    this.$nextTick(function () {\n      _this.initializeCharts();\n    });\n  },\n  methods: {\n    initializeCharts: function initializeCharts() {\n      var chartElement = document.getElementById('barChart');\n      if (chartElement && !this.chart) {\n        var ctx = chartElement.getContext('2d');\n        this.chart = new Chart(ctx, {\n          type: 'bar',\n          data: this.chartData,\n          options: {\n            responsive: true,\n            maintainAspectRatio: false,\n            scales: {\n              y: {\n                beginAtZero: true,\n                grid: {\n                  color: 'rgba(255, 255, 255, 0.1)'\n                },\n                ticks: {\n                  color: 'rgba(255, 255, 255, 0.7)',\n                  font: {\n                    size: 12\n                  }\n                }\n              },\n              x: {\n                grid: {\n                  color: 'rgba(255, 255, 255, 0.1)'\n                },\n                ticks: {\n                  color: 'rgba(255, 255, 255, 0.7)',\n                  font: {\n                    size: 12\n                  }\n                }\n              }\n            },\n            plugins: {\n              legend: {\n                display: false\n              },\n              tooltip: {\n                backgroundColor: 'rgba(0, 0, 0, 0.8)',\n                titleColor: '#fff',\n                bodyColor: '#fff',\n                borderColor: 'rgba(255, 255, 255, 0.1)',\n                borderWidth: 1,\n                padding: 10,\n                displayColors: false\n              }\n            },\n            animation: {\n              duration: 2000,\n              easing: 'easeInOutQuart'\n            }\n          }\n        });\n      }\n    }\n  }\n};","map":{"version":3,"names":["DangerousHTML","Chart","name","props","components","data","rawv1yq","chart","chartData","labels","datasets","label","backgroundColor","borderColor","borderWidth","metaInfo","title","meta","property","content","mounted","_this","$nextTick","initializeCharts","methods","chartElement","document","getElementById","ctx","getContext","type","options","responsive","maintainAspectRatio","scales","y","beginAtZero","grid","color","ticks","font","size","x","plugins","legend","display","tooltip","titleColor","bodyColor","padding","displayColors","animation","duration","easing"],"sources":["src/views/page-03.vue"],"sourcesContent":["<template>\n  <div class=\"page03-container10\">\n    <div class=\"page03-container11\">\n      <h1 class=\"page03-text10\">Officer - 01</h1>\n      <div class=\"page03-container12\"></div>\n      <span class=\"page03-text11\">Department :   XYZ</span>\n      <h1 class=\"page03-text12\">Civilinc</h1>\n      <a\n        href=\"https://example.com\"\n        target=\"_blank\"\n        rel=\"noreferrer noopener\"\n        class=\"page03-link1\"\n      >\n        Log out\