icer?.id === officer.id }\"\r\n            @click=\"selectOfficer(officer)\"\r\n          >\r\n            <div class=\"officer-info\">\r\n              <div class=\"officer-name\">{{ officer.name }}</div>\r\n              <div class=\"officer-details\">\r\n                <span class=\"officer-role\">{{ officer.role }}</span>\r\n                <span class=\"officer-cases\">Active Cases: {{ officer.active_cases }}</span>\r\n              </div>\r\n            </div>\r\n            <div class=\"officer-status\" :class=\"officer.status\">\r\n              {{ officer.status }}\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"no-results\" v-else>\r\n          No officers found matching your criteria\r\n        </div>\r\n\r\n        <div class=\"action-buttons\">\r\n          <button \r\n            class=\"assign-button\"\r\n            :disabled=\"!selectedOfficer\"\r\n            @click=\"assignOfficer\"\r\n          >\r\n            <i class=\"fas fa-user-plus\"></i>\r\n            Assign Officer\r\n          </button>\r\n          <button class=\"cancel-button\" @click=\"$emit('close')\">\r\n            Cancel\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'AssignOfficerModal',\r\n  props: {\r\n    complaint: {\r\n      type: Object,\r\n      required: true\r\n    },\r\n    departments: {\r\n      type: Array,\r\n      required: true\r\n    },\r\n    officers: {\r\n      type: Array,\r\n      required: true\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      selectedDepartment: '',\r\n      searchQuery: '',\r\n      selectedOfficer: null,\r\n      filteredOfficers: []\r\n    }\r\n  },\r\n  created() {\r\n    this.filterOfficers()\r\n  },\r\n  methods: {\r\n    filterOfficers() {\r\n      this.filteredOfficers = this.officers.filter(officer => {\r\n        const matchesDepartment = !this.selectedDepartment || \r\n          officer.