isplay: flex;\n  flex-direction: column;\n  gap: 0.25rem;\n}\n\n.author-name {\n  font-size: 1.2rem;\n  font-weight: 600;\n  color: #00ff87;\n}\n\n.author-title {\n  font-size: 1rem;\n  color: #60efff;\n  font-weight: 500;\n}\n\n.author-org {\n  font-size: 0.9rem;\n  color: rgba(255, 255, 255, 0.9);\n}\n\n.author-location {\n  font-size: 0.85rem;\n  color: rgba(255, 255, 255, 0.7);\n}\n\n.testimonial-text {\n  font-size: 1.1rem;\n  line-height: 1.6;\n  color: rgba(255, 255, 255, 0.9);\n  flex-grow: 1;\n}\n\n/* Animation Classes */\n.animate-text {\n  animation: textReveal 1s ease-out forwards;\n  opacity: 0;\n  transform: translateY(20px);\n}\n\n.animate-fade {\n  animation: fadeIn 1s ease-out 0.5s forwards;\n  opacity: 0;\n}\n\n.animate-bounce {\n  animation: bounceIn 1s ease-out 1s forwards;\n  opacity: 0;\n}\n\n.animate-slide {\n  animation: slideIn 1s ease-out forwards;\n  opacity: 0;\n}\n\n.animate-fade-up {\n  animation: fadeUp 1s ease-out forwards;\n  opacity: 0;\n}\n\n/* Animation Keyframes */\n@keyframes textReveal {\n  0% {\n    opacity: 0;\n    transform: translateY(20px);\n  }\n  100% {\n    opacity: 1;\n    transform: translateY(0);\n  }\n}\n\n@keyframes fadeIn {\n  0% {\n    opacity: 0;\n  }\n  100% {\n    opacity: 1;\n  }\n}\n\n@keyframes bounceIn {\n  0% {\n    opacity: 0;\n    transform: scale(0.8);\n  }\n  50% {\n    transform: scale(1.1);\n  }\n  100% {\n    opacity: 1;\n    transform: scale(1);\n  }\n}\n\n@keyframes slideIn {\n  0% {\n    opacity: 0;\n    transform: translateX(-30px);\n  }\n  100% {\n    opacity: 1;\n    transform: translateX(0);\n  }\n}\n\n@keyframes fadeUp {\n  0% {\n    opacity: 0;\n    transform: translateY(30px);\n  }\n  100% {\n    opacity: 1;\n    transform: translateY(0);\n  }\n}\n\n/* Fluid Distortion Effect */\n@keyframes fluidDistortion {\n  0% {\n    transform: translate(-50%, -50%) rotate(0deg) scale(1);\n  }\n  50% {\n    transform: translate(-50%, -50%) rotate(180deg) scale(1.1);\n  }\n  100% {\n    transform: translate(-50%, -50%) rotate(360deg) scale(1);\n  }\n}\n\n.fluid-effect {\n  position: absolute;\n  top: 50%;\n  left: 50%;\n  width: 150%;\n  height: 150%;\n  background: radial-gradient(circle, rgba(0,255,135,0.1) 0%, rgba(96,239,255,0.1) 100%);\n  animation: fluidDistortion 20s infinite linear;\n  pointer-events: none;\n  z-index: 0;\n}\n</style>\n"],"mappings":"AA6EA,OAAAA,WAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IACAF,WAAA,EAAAA;EACA;EACAG,IAAA,WAAAA,KAAA;IACA;MACA;IAAA,CACA;EACA;EACAC,OAAA,WAAAA,QAAA;IACA;IACA,KAAAC,qBAAA;EACA;EACAC,OAAA;IACAD,qBAAA,WAAAA,sBAAA;MACA;IAAA;EAEA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}