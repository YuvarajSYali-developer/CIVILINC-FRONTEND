{"ast":null,"code":"export default {\n  name: 'ComplaintModal',\n  props: {\n    complaint: {\n      type: Object,\n      required: true\n    },\n    isCommissioner: {\n      type: Boolean,\n      default: false\n    }\n  },\n  data: function data() {\n    return {\n      newStatus: this.complaint.status\n    };\n  },\n  methods: {\n    formatDate: function formatDate(date) {\n      return new Date(date).toLocaleDateString('en-IN');\n    },\n    updateStatus: function updateStatus() {\n      this.$emit('update-status', {\n        complaintId: this.complaint.id,\n        newStatus: this.newStatus\n      });\n    },\n    editComplaint: function editComplaint() {\n      this.$emit('edit', this.complaint);\n    },\n    viewFile: function viewFile(file) {\n      // Implement file viewing logic\n      console.log('Viewing file:', file);\n    }\n  }\n};","map":{"version":3,"names":["name","props","complaint","type","Object","required","isCommissioner","Boolean","default","data","newStatus","status","methods","formatDate","date","Date","toLocaleDateString","updateStatus","$emit","complaintId","id","editComplaint","viewFile","file","console","log"],"sources":["src/components/ComplaintModal.vue"],"sourcesContent":["<template>\r\n  <div class=\"modal-overlay\" @click=\"$emit('close')\">\r\n    <div class=\"modal-content\" @click.stop>\r\n      <div class=\"modal-header\">\r\n        <h2>Complaint Details</h2>\r\n        <button class=\"close-button\" @click=\"$emit('close')\">\r\n          <i class=\"fas fa-times\"></i>\r\n        </button>\r\n      </div>\r\n\r\n      <div class=\"complaint-details\">\r\n        <div class=\"detail-grid\">\r\n          <div class=\"detail-group\">\r\n            <label>Complaint ID</label>\r\n            <span>{{ complaint.id }}</span>\r\n          </div>\r\n\r\n          <div class=\"detail-group\">\r\n            <label>Status</label>\r\n            <span :class=\"['status-badge', complaint.status]\">\r\n              {{ complaint.status }}\r\n            </span>\r\n          </div>\r\n\r\n          <div class=\"detail-group\">\r\n            <label>Priority</label>\r\n            <span :class=\"['priority-badge', complaint.priority]\">\r\n              {{ complaint.priority }}\r\n            </span>\r\n          </div>\r\n\r\n          <div class=\"detail-group\">\r\n            <label>Department</label>\r\n            <span>{{ complaint.department }}</span>\r\n          </div>\r\n\r\n          <div class=\"detail-group\">\r\n            <label>Date Submitted</label>\r\n            <span>{{ formatDate(complaint.date_submitted) }}</span>\r\n          </div>\r\n\r\n          <div class=\"detail-group\">\r\n            <label>Assigned Officer</label>\r\n            <span>{{ complaint.assigned_officer || 'Not Assigned' }}</span>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"detail-section\">\r\n          <h3>Citizen Information</h3>\r\n          <div class=\"detail-grid\">\r\n            <div class=\"detail-group\">\r\n              <label>Name</label>\r\n              <span>{{ complaint.citizen_name }}</span>\r\n            </div>\r\n            <div class=\"detail-group\">\r\n              <label>Contact</label>\r\n              <span>{{ complaint.contact }}</span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"detail-section\">\r\n          <h3>Complaint Description</h3>\r\n          <p class=\"description\">{{ complaint.description }}</p>\r\n        </div>\r\n\r\n        <div class=\"detail-section\" v-if=\"complaint.attachments && complaint.attachments.length\">\r\n          <h3>Attachments</h3>\r\n          <div class=\"attachments-list\">\r\n            <div v-for=\"(file, index) in complaint.attachments\" :key=\"index\" class=\"file-tag\">\r\n              <i class=\"fas fa-file\"></i>\r\n              {{ file.name }}\r\n              <button class=\"view-file\" @click=\"viewFile(file)\">\r\n                <i class=\"fas fa-eye\"></i>\r\n              </button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"detail-section\">\r\n          <h3>Resolution Notes</h3>\r\n          <div class=\"resolution-notes\">\r\n            <div v-if=\"complaint.resolution_notes\" class=\"notes-content\">\r\n              {{ complaint.resolution_notes }}\r\n            </div>\r\n            <div v-else class=\"no-notes\">\r\n              No resolution notes available\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"action-section\">\r\n          <div class=\"status-update\" v-if=\"isCommissioner\">\r\n            <label for=\"status\">Update Status</label>\r\n            <select id=\"status\" v-model=\"newStatus\" @change=\"updateStatus\">\r\n              <option value=\"unresolved\">Unresolved</option>\r\n              <option value=\"assigned\">Assigned</option>\r\n              <option value=\"in-progress\">In Progress</option>\r\n              <option value=\"resolved\">Resolved</option>\r\n            </select>\r\n          </div>\r\n\r\n          <div class=\"action-buttons\">\r\n            <button \r\n              v-if=\"isCommissioner && !complaint.assigned_officer\"\r\n              class=\"assign-button\"\r\n              @click=\"$emit('assign')\"\r\n            >\r\n              <i class=\"fas fa-user-plus\"></i>\r\n              Assign Officer\r\n            </button>\r\n            <button \r\n              v-if=\"isCommissioner\"\r\n              class=\"edit-button\"\r\n              @click=\"editComplaint\"\r\n            >\r\n              <i class=\"fas fa-edit\"></i>\r\n              Edit Complaint\r\n            </button>\r\n            <button class=\"close-button\" @click=\"$emit('close')\">\r\n              Close\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'ComplaintModal',\r\n  props: {\r\n    complaint: {\r\n      type: Object,\r\n      required: true\r\n    },\r\n    isCommissioner: {\r\n      type: Boolean,\r\n      default: false\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      newStatus: this.complaint.status\r\n    }\r\n  },\r\n  methods: {\r\n    formatDate(date) {\r\n      return new Date(date).toLocaleDateString('en-IN')\r\n    },\r\n    updateStatus() {\r\n      this.$emit('update-status', {\r\n        complaintId: this.complaint.id,\r\n        newStatus: this.newStatus\r\n      })\r\n    },\r\n    editComplaint() {\r\n      this.$emit('edit', this.complaint)\r\n    },\r\n    viewFile(file) {\r\n      // Implement file viewing logic\r\n      console.log('Viewing file:', file)\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.modal-overlay {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n  background: rgba(0, 0, 0, 0.5);\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  z-index: 1000;\r\n}\r\n\r\n.modal-content {\r\n  background: #ffffff;\r\n  border-radius: 12px;\r\n  width: 90%;\r\n  max-width: 800px;\r\n  max-height: 90vh;\r\n  overflow-y: auto;\r\n  padding: 2rem;\r\n}\r\n\r\n.modal-header {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  margin-bottom: 2rem;\r\n  padding-bottom: 1rem;\r\n  border-bottom: 2px solid rgba(37, 99, 235, 0.1);\r\n}\r\n\r\n.modal-header h2 {\r\n  color: #1e40af;\r\n  font-size: 1.5rem;\r\n  font-weight: 600;\r\n}\r\n\r\n.close-button {\r\n  background: none;\r\n  border: none;\r\n  color: #4b5563;\r\n  font-size: 1.2rem;\r\n  cursor: pointer;\r\n  padding: 0.5rem;\r\n  border-radius: 4px;\r\n  transition: all 0.3s ease;\r\n}\r\n\r\n.close-button:hover {\r\n  background: rgba(37, 99, 235, 0.1);\r\n  color: #2563eb;\r\n}\r\n\r\n.complaint-details {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 2rem;\r\n}\r\n\r\n.detail-grid {\r\n  display: grid;\r\n  grid-template-columns: 