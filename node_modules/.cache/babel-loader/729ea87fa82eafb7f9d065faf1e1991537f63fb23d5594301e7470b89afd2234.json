{"ast":null,"code":"import DangerousHTML from 'dangerous-html/vue';\nimport Chart from 'chart.js';\nexport default {\n  name: 'Page03',\n  props: {},\n  components: {\n    DangerousHTML: DangerousHTML\n  },\n  data: function data() {\n    return {\n      rawv1yq: ' ',\n      chart: null\n    };\n  },\n  metaInfo: {\n    title: 'Page-03 - civilinc',\n    meta: [{\n      property: 'og:title',\n      content: 'Page-03 - civilinc'\n    }]\n  },\n  mounted: function mounted() {\n    var _this = this;\n    this.$nextTick(function () {\n      _this.initializeCharts();\n    });\n  },\n  methods: {\n    initializeCharts: function initializeCharts() {\n      var chartElement = document.getElementById('barChart');\n      if (chartElement && !this.chart) {\n        var ctx = chartElement.getContext('2d');\n        this.chart = new Chart(ctx, {\n          type: 'bar',\n          data: {\n            labels: ['Roads', 'Water', 'Electricity', 'Sanitation', 'Other'],\n            datasets: [{\n              label: 'Complaints by Category',\n              data: [45, 30, 25, 20, 15],\n              backgroundColor: ['rgba(0, 255, 135, 0.8)', 'rgba(96, 239, 255, 0.8)', 'rgba(255, 99, 132, 0.8)', 'rgba(54, 162, 235, 0.8)', 'rgba(255, 206, 86, 0.8)'],\n              borderColor: ['rgba(0, 255, 135, 1)', 'rgba(96, 239, 255, 1)', 'rgba(255, 99, 132, 1)', 'rgba(54, 162, 235, 1)', 'rgba(255, 206, 86, 1)'],\n              borderWidth: 1\n            }]\n          },\n          options: {\n            responsive: true,\n            maintainAspectRatio: false,\n            scales: {\n              y: {\n                beginAtZero: true,\n                grid: {\n                  color: 'rgba(255, 255, 255, 0.1)'\n                },\n                ticks: {\n                  color: 'rgba(255, 255, 255, 0.7)'\n                }\n              },\n              x: {\n                grid: {\n                  color: 'rgba(255, 255, 255, 0.1)'\n                },\n                ticks: {\n                  color: 'rgba(255, 255, 255, 0.7)'\n                }\n              }\n            },\n            plugins: {\n              legend: {\n                labels: {\n                  color: 'rgba(255, 255, 255, 0.7)'\n                }\n              }\n            }\n          }\n        });\n      }\n    }\n  }\n};","map":{"version":3,"names":["DangerousHTML","Chart","name","props","components","data","rawv1yq","chart","metaInfo","title","meta","property","content","mounted","_this","$nextTick","initializeCharts","methods","chartElement","document","getElementById","ctx","getContext","type","labels","datasets","label","backgroundColor","borderColor","borderWidth","options","responsive","maintainAspectRatio","scales","y","beginAtZero","grid","color","ticks","x","plugins","legend"],"sources":["src/views/page-03.vue"],"sourcesContent":["<template>\n  <div class=\"page03-container10\">\n    <div class=\"page03-container11\">\n      <h1 class=\"page03-text10\">Officer - 01</h1>\n      <div class=\"page03-container12\"></div>\n      <span class=\"page03-text11\">Department :   XYZ</span>\n      <h1 class=\"page03-text12\">Civilinc</h1>\n      <a\n        href=\"https://example.com\"\n        target=\"_blank\"\n        rel=\"noreferrer noopener\"\n        class=\"page03-link1\"\n      >\n        Log out\n      </a>\n      <a\n        href=\"https://example.com\"\n        target=\"_blank\"\n        rel=\"noreferrer noopener\"\n        class=\"page03-link2\"\n      >\n        Complaints\n      </a>\n      <a\n        href=\"https://example.com\"\n        target=\"_blank\"\n        rel=\"noreferrer noopener\"\n        class=\"page03-link3\"\n      >\n        Forums\n      </a>\n      <a\n        href=\"https://example.com\"\n        target=\"_blank\"\n        rel=\"noreferrer noopener\"\n        class=\"page03-link4\"\n      >\n        Dashboard\n      </a>\n      <a\n        href=\"https://example.com\"\n        target=\"_blank\"\n        rel=\"noreferrer noopener\"\n        class=\"page03-link5\"\n      >\n        Projects\n      </a>\n    </div>\n    <div class=\"page03-container13\">\n      <animate-on-reveal\n        animation=\"fadeIn\"\n        duration=\"500ms\"\n        delay=\"0s\"\n        direction=\"normal\"\n        easing=\"ease\"\n        iteration=\"1\"\n      >\n        <div data-thq-animate-on-reveal=\"true\" class=\"page03-container14\">\n          <span class=\"page03-text13\">300</span>\n          <h1 class=\"page03-text14\">\n            Total  Projects\n            <span v-html=\"rawv1yq\"></span>\n          </h1>\n          <span class=\"page03-text15\">12% Increased</span>\n        </div>\n      </animate-on-reveal>\n      <svg width=\"32\" height=\"32\" viewBox=\"0 0 32 32\" class=\"page03-icon1\">\n        <g fill=\"none\">\n          <path\n            d=\"M2 6a4 4 0 0 1 4-4h20a4 4 0 0 1 4 4v20a4 4 0 0 1-4 4H6a4 4 0 0 1-4-4z\"\n            fill=\"#00D26A\"\n          ></path>\n          <path\n            d=\"M5.461 5.119a.75.75 0 0 1 1.014.31l2.068 3.9l2.206-3.916a.75.75 0 1 1 1.307.737l-2.402 4.26h1.749a.75.75 0 1 1 0 1.5h-2.13v.95h2.13a.75.75 0 0 1 0 1.5h-2.13v1.4a.75.75 0 0 1-1.5 0v-1.4h-1.82a.75.75 0 0 1 0-1.5h1.82v-.95h-1.82a.75.75 0 0 1 0-1.5h1.466l-2.27-4.278a.75.75 0 0 1 .312-1.014m21.328 9.326a.75.75 0 0 0-1.06-1.06L15.304 23.809a.25.25 0 0 1-.355-.001l-2.617-2.637a1.75 1.75 0 0 0-2.48-.005l-4.421 4.422a.75.75 0 0 0 1.06 1.06l4.422-4.422a.25.25 0 0 1 .354.001l2.618 2.637a1.75 1.75 0 0 0 2.48.005z\"\n            fill=\"#F4F4F4\"\n          ></path>\n        </g>\n      </svg>\n      <animate-on-reveal\n        animation=\"fadeIn\"\n        duration=\"500ms\"\n        delay=\"0s\"\n        direction=\"normal\"\n        easing=\"ease\"\n        iteration=\"1\"\n      >\n        <div data-thq-animate-on-reveal=\"true\" class=\"page03-container15\">\n          <span class=\"page03-text16\">240</span>\n          <span class=\"page03-text17\">↑12% since last week</span>\n          <h1 class=\"page03-text18\">Complaints resolved</h1>\n          <svg width=\"32\" height=\"32\" viewBox=\"0 0 32 32\" class=\"page03-icon5\">\n            <g fill=\"none\">\n              <path\n                d=\"M2 6a4 4 0 0 1 4-4h20a4 4 0 0 1 4 4v20a4 4 0 0 1-4 4H6a4 4 0 0 1-4-4z\"\n                fill=\"#00D26A\"\n              ></path>\n              <path\n                d=\"M5.461 5.119a.75.75 0 0 1 1.014.31l2.068 3.9l2.206-3.916a.75.75 0 1 1 1.307.737l-2.402 4.26h1.749a.75.75 0 1 1 0 1.5h-2.13v.95h2.13a.75.75 0 0 1 0 1.5h-2.13v1.4a.75.75 0 0 1-1.5 0v-1.4h-1.82a.75.75 0 0 1 0-1.5h1.82v-.95h-1.82a.75.75 0 0 1 0-1.5h1.466l-2.27-4.278a.75.75 0 0 1 .312-1.014m21.328 9.326a.75.75 0 0 0-1.06-1.06L15.304 23.809a.25.25 0 0 1-.355-.001l-2.617-2.637a1.75 1.75 0 0 0-2.48-.005l-4.421 4.422a.75.75 0 0 0 1.06 1.06l4.422-4.422a.25.25 0 0 1 .354.001l2.618 2.637a1.75 1.75 0 0 0 2.48.005z\"\n                fill=\"#F4F4F4\"\n              ></path>\n            </g>\n          </svg>\n        </div>\n      </animate-on-reveal>\n      <animate-on-reveal\n        animation=\"fadeIn\"\n        duration=\"500ms\"\n        delay=\"0s\"\n        direction=\"normal\"\n        easing=\"ease\"\n        iteration=\"1\"\n      >\n        <div data-thq-animate-on-reveal=\"true\" class=\"page03-container16\">\n          <h1 class=\"page03-text19\">Avg response time</h1>\n          <span class=\"page03-text20\">06 (hrs) : 13 (mins)</span>\n          <span class=\"page03-text21\">↓5% vs last month</span>\n        </div>\n      </animate-on-reveal>\n      <animate-on-reveal\n        animation=\"fadeIn\"\n        duration=\"500ms\"\n        delay=\"0s\"\n        direction=\"normal\"\n        easing=\"ease\"\n        iteration=\"1\"\n      >\n        <div data-thq-animate-on-reveal=\"true\" class=\"page03-container17\">\n          <span class=\"page03-text22\">124</span>\n          <h1 class=\"page03-text23\">✅ Projects Completed</h1>\n          <span class=\"page03-text24\">↑3% vs last week</span>\n        </div>\n      </animate-on-reveal>\n      <animate-on-reveal\n        animation=\"fadeIn\"\n        delay=\"0s\"\n        direction=\"normal\"\n        easing=\"ease\"\n        iteration=\"1\"\n      >\n        <h1 data-thq-animate-on-reveal=\"true\" class=\"page03-text25\">\n          Key Progress indicators\n        </h1>\n      </animate-on-reveal>\n    </div>\n    <div class=\"page03-container18\">\n      <div class=\"page03-container19\">\n        <DangerousHTML\n          html=\"<div style='width: 100%; overflow-x: auto; font-family: 'Inter', sans-serif;'>\n    <table style='width: 100%; border-collapse: collapse; font-size: 14px; color: #111827; border: 1px solid #6b7280;'>\n      <thead style='background-color: #f3f4f6;'>\n        <tr>\n          <th style='padding: 12px; border: 1px solid #6b7280;'>Project ID</th>\n          <th style='padding: 12px; border: 1px solid #6b7280;'>Project Name</th>\n          <th style='padding: 12px; border: 1px solid #6b7280;'>Department</th>\n          <th style='padding: 12px; border: 1px solid #6b7280;'>Start Date</th>\n          <th style='padding: 12px; border: 1px solid #6b7280;'>Status</th>\n          <th style='padding: 12px; border: 1px solid #6b7280;'>Location</th>\n        </tr>\n      </thead>\n      <tbody>\n        <tr>\n          <td style='padding: 12px; border: 1px solid #6b7280;'>KA-201</td>\n          <td style='padding: 12px; border: 1px solid #6b7280;'>Smart Traffic Signals</td>\n          <td style='padding: 12px; border: 1px solid #6b7280;'>Transport</td>\n          <td style='padding: 12px; border: 1px solid #6b7280;'>2024-11-10</td>\n          <td style='padding: 12px; border: 1px solid #6b7280; color: #10b981;'>Active</td>\n          <td style='padding: 12px; border: 1px solid #6b7280;'>\n            <a href='https://maps.google.com/?q=19.0760,72.8777' target='_blank' style='color: #3b82f6; text-decoration: underline;'>Shimoga</a>\n          </td>\n        </tr>\n        <tr style='background-color: #f9fafb;'>\n          <td style='padding: 12px; border: 1px solid #6b7280;'>KA-118</td>\n          <td style='padding: 12px; border: 1px solid #6b7280;'>Public Wi-Fi Zones</td>\n          <td style='padding: 12px; border: 1px solid #6b7280;'>IT</td>\n          <td style='padding: 12px; border: 1px solid #6b7280;'>2024-09-01</td>\n          <td style='padding: 12px; border: 1px solid #6b7280; color: #f59e0b;'>Pending</td>\n          <td style='padding: 12px; border: 1px solid #6b7280;'>\n            <a href='https://maps.google.com/?q=12.9716,77.5946' target='_blank' style='color: #3b82f6; text-decoration: underline;'>Bangalore</a>\n          </td>\n        </tr>\n        <tr>\n          <td style='padding: 12px; border: 1px solid #6b7280;'>KA-022</td>\n          <td style='padding: 12px; border: 1px solid #6b7280;'>Waste Sensor Grid</td>\n          <td style='padding: 12px; border: 1px solid #6b7280;'>Sanitation</td>\n          <td style='padding: 12px; border: 1px solid #6b7280;'>2023-12-20</td>\n          <td style='padding: 12px; border: 1px solid #6b7280; color: #ef4444;'>Delayed</td>\n          <td style='padding: 12px; border: 1px solid #6b7280;'>\n            <a href='https://maps.google.com/?q=28.6139,77.2090' target='_blank' style='color: #3b82f6; text-decoration: underline;'>Udupi</a>\n          </td>\n        </tr>\n        <tr style='background-color: #f9fafb;'>\n          <td style='padding: 12px; border: 1px solid #6b7280;'>KA-075</td>\n          <td style='padding: 12px; border: 1px solid #6b7280;'>Rainwater Harvest Monitoring</td>\n          <td style='padding: 12px; border: 1px solid #6b7280;'>Environment</td>\n          <td style='padding: 12px; border: 1px solid #6b7280;'>2024-08-05</td>\n          <td style='padding: 12px; border: 1px solid #6b7280; color: #10b981;'>Active</td>\n          <td style='padding: 12px; border: 1px solid #6b7280;'>\n            <a href='https://maps.google.com/?q=13.0827,80.2707' target='_blank' style='color: #3b82f6; text-decoration: underline;'>Mysore</a>\n          </td>\n        </tr>\n      </tbody>\n    </table>\n  </div>\n  \"\n        ></DangerousHTML>\n      </div>\n    </div>\n    <div class=\"page03-container20\">\n      <div class=\"page03-container21\">\n        <DangerousHTML\n          html=\"<div style='position: relative; width: 100%; max-width: 800px; margin: auto;'>\n  \n    <!-- Simulated Zoom Controls -->\n    <div style='position: absolute; top: 10px; left: 10px; z-index: 10;'>\n      <button style='padding: 6px 10px; margin-bottom: 5px; font-weight: bold; background-color: #fff; border: 1px solid #ccc; border-radius: 4px; cursor: pointer;'>+</button><br>\n      <button style='padding: 6px 10px; font-weight: bold; background-color: #fff; border: 1px solid #ccc; border-radius: 4px; cursor: pointer;'>−</button>\n    </div>\n  \n    <!-- Google Maps iframe -->\n    <iframe \n      src='https://www.google.com/maps/d/embed?mid=1nbxZjWIZf5rc4ZZUjIudC0WSKQ1IoiQ&ehbc=2E312F' \n      width='100%' \n      height='480' \n      style='border: 3px solid #333; border-radius: 12px;'\n      allowfullscreen>\n    </iframe>\n  \n    <!-- External Map Link Button -->\n    <div style='text-align: center; margin-top: 10px;'>\n      <a \n        href='https://www.google.com/maps/d/viewer?mid=1nbxZjWIZf5rc4ZZUjIudC0WSKQ1IoiQ&hl=en' \n        target='_blank' \n        style='display: inline-block; padding: 10px 20px; background-color: #1a73e8; color: white; text-decoration: none; border-radius: 6px; font-weight: bold;'>\n        View Full Map in Google Maps\n      </a>\n    </div>\n  \n  </div>\n  \"\n        ></DangerousHTML>\n      </div>\n    </div>\n    <div class=\"page03-container22\">\n      <div class=\"page03-container23\">\n        <DangerousHTML\n          html=\"<style>\n    #complaintsSection {\n      width: 784px;\n      height: 283px;\n      overflow: auto;\n      font-family: Arial, sans-serif;\n      background: #f9f9f9;\n      padding: 10px;\n      box-sizing: border-box;\n      border: 1px solid #ccc;\n      border-radius: 8px;\n    }\n    table {\n      width: 100%;\n      border-collapse: collapse;\n      font-size: 12px;\n      margin-bottom: 6px;\n    }\n    th, td {\n      border: 1.5px solid #333;\n      padding: 4px 6px;\n      text-align: left;\n    }\n    th {\n      background-color: #222;\n      color: white;\n    }\n    select, button {\n      padding: 4px 10px;\n      font-weight: 600;\n      font-size: 12px;\n      border-radius: 4px;\n      border: 1px solid #666;\n      cursor: pointer;\n    }\n    textarea {\n      font-size: 11px;\n      resize: none;\n      width: 100%;\n      height: 40px;\n    }\n    .respond-section {\n      display: flex;\n      flex-direction: column;\n    }\n    .respond-btn {\n      margin-top: 4px;\n      background-color: #1a73e8;\n      color: white;\n      border: none;\n      font-size: 12px;\n      padding: 4px;\n    }\n  </style>\n  \n  <div id='complaintsSection'>\n    <h3 style='margin:0 0 6px 0; font-size: 16px;'>📢 Public Complaints Summary</h3>\n  \n    <label for='statusFilter' style='font-size: 12px;'>Filter by status: </label>\n    <select id='statusFilter' onchange='filterTable()' style='margin-bottom: 6px;'>\n      <option value='All'>All</option>\n      <option value='Pending'>Pending</option>\n      <option value='Escalated'>Escalated</option>\n    </select>\n  \n    <table id='complaintsTable'>\n      <thead>\n        <tr>\n          <th>ID</th>\n          <th>Location</th>\n          <th>Issue</th>\n          <th>Status</th>\n          <th>Dept.</th>\n          <th>Respond</th>\n        </tr>\n      </thead>\n      <tbody>\n        <tr>\n          <td>CMP1001</td>\n          <td>Sector 5</td>\n          <td>Road Damage</td>\n          <td>Pending</td>\n          <td>Public Works</td>\n          <td class='respond-section'>\n            <textarea id='comment-CMP1001' placeholder='Add response...'></textarea>\n            <button class='respond-btn' onclick='submitResponse('CMP1001')'>Submit</button>\n          </td>\n        </tr>\n        <tr>\n          <td>CMP1002</td>\n          <td>Park Avenue</td>\n          <td>Water Leakage</td>\n          <td>Escalated</td>\n          <td>Water Dept.</td>\n          <td class='respond-section'>\n            <textarea id='comment-CMP1002' placeholder='Add response...'></textarea>\n            <button class='respond-btn' onclick='submitResponse('CMP1002')'>Submit</button>\n          </td>\n        </tr>\n        <tr>\n          <td>CMP1003</td>\n          <td>Main Street</td>\n          <td>Electricity Outage</td>\n          <td>Resolved</td>\n          <td>Electricity Dept.</td>\n          <td class='respond-section'>\n            <textarea id='comment-CMP1003' placeholder='Add response...'></textarea>\n            <button class='respond-btn' onclick='submitResponse('CMP1003')'>Submit</button>\n          </td>\n        </tr>\n        <tr>\n          <td>CMP1004</td>\n          <td>Downtown</td>\n          <td>Garbage Collection</td>\n          <td>Pending</td>\n          <td>Sanitation Dept.</td>\n          <td class='respond-section'>\n            <textarea id='comment-CMP1004' placeholder='Add response...'></textarea>\n            <button class='respond-btn' onclick='submitResponse('CMP1004')'>Submit</button>\n          </td>\n        </tr>\n      </tbody>\n    </table>\n  \n    <h4 style='margin: 6px 0 4px 0; font-size: 14px;'>Complaints by Category</h4>\n    <canvas id='barChart' width='750' height='120'></canvas>\n  </div>\n  \n  <script src='https://cdn.jsdelivr.net/npm/chart.js'></script>\n  <script>\n    function filterTable() {\n      const filter = document.getElementById('statusFilter').value;\n      const rows = document.querySelectorAll('#complaintsTable tbody tr');\n  \n      rows.forEach(row => {\n        const status = row.cells[3].textContent;\n        row.style.display = (filter === 'All' || status === filter) ? '' : 'none';\n      });\n    }\n  \n    funct