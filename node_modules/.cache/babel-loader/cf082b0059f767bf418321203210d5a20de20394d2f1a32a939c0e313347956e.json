;\n      files.forEach(function (file) {\n        _this.formData.documents.push({\n          name: file.name,\n          file: file\n        });\n      });\n    },\n    removeFile: function removeFile(index) {\n      this.formData.documents.splice(index, 1);\n    }\n  }\n};","map":{"version":3,"names":["name","props","project","type","Object","default","data","formData","title","department","status","start_date","end_date","budget","description","assigned_engineers","documents","newEngineer","departments","watch","immediate","handler","newProject","_objectSpread","methods","handleSubmit","$emit","addEngineer","trim","push","removeEngineer","index","splice","handleFileUpload","event","_this","files","Array","from","target","forEach","file","removeFile"],"sources":["src/components/ProjectModal.vue"],"sourcesContent":["<template>\r\n  <div class=\"modal-overlay\" @click=\"$emit('close')\">\r\n    <div class=\"modal-content\" @click.stop>\r\n      <div class=\"modal-header\">\r\n        <h2>{{ project ? 'Edit Project' : 'Add New Project' }}</h2>\r\n        <button class=\"close-button\" @click=\"$emit('close')\">\r\n          <i class=\"fas fa-times\"></i>\r\n        </button>\r\n      </div>\r\n\r\n      <form @submit.prevent=\"handleSubmit\" class=\"project-form\">\r\n        <div class=\"form-grid\">\r\n          <div class=\"form-group\">\r\n            <label for=\"title\">Project Title</label>\r\n            <input \r\n              type=\"text\" \r\n              id=\"title\" \r\n              v-model=\"formData.title\" \r\n              required\r\n              placeholder=\"Enter project title\"\r\n            >\r\n          </div>\r\n\r\n          <div class=\"form-group\">\r\n            <label for=\"department\">Department</label>\r\n            <select id=\"department\" v-model=\"formData.department\" required>\r\n              <option value=\"\">Select Department</option>\r\n              <option v-for=\"dept in departments\" :key=\"dept\" :value=\"dept\">\r\n                {{ dept }}\r\n              </option>\r\n            </select>\r\n          </div>\r\n\r\n          <div class=\"form-group\">\r\n            <label for=\"status\">Status</label>\r\n            <select id=\"status\" v-model=\"formData.status\" required>\r\n              <option value=\"planned\">Planned</option>\r\n              <option value=\"in-progress\">In Progress</option>\r\n              <option value=\"completed\">Completed</option>\r\n            </select>\r\n          </div>\r\n\r\n          <div class=\"form-group\">\r\n            <label for=\"start_date\">Start Date</label>\r\n            <input \r\n              type=\"date\" \r\n              id=\"start_date\" \r\n              v-model=\"formData.start_date\" \r\n              required\r\n            >\r\n          </div>\r\n\r\n          <div class=\"form-group\">\r\n            <label for=\"end_date\">End Date</label>\r\n            <input \r\n              type=\"date\" \r\n              id=\"end_date\" \r\n              v-model=\"formData.end_date\" \r\n              required\r\n            >\r\n          </div>\r\n\r\n          <div class=\"form-group\">\r\n            <label for=\"budget\">Budget (â‚¹)</label>\r\n            <input \r\n              type=\"number\" \r\n              id=\"budget\" \r\n              v-model=\"formData.budget\" \r\n              placeholder=\"Enter budget amount\"\r\n            >\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"form-group full-width\">\r\n          <label for=\"description\">Description</label>\r\n          <textarea \r\n            id=\"description\" \r\n            v-model=\"formData.description\" \r\n            rows=\"4\" \r\n            required\r\n            placeholder=\"Enter project description\"\r\n          ></textarea>\r\n        </div>\r\n\r\n        <div class=\"form-group full-width\">\r\n          <label for=\"engineers\">Assigned Engineers</label>\r\n          <div class=\"engineers-list\">\r\n            <div v-for=\"(engineer, index) in formData.assigned_engineers\" :key=\"index\" class=\"engineer-tag\">\r\n              {{ engineer }}\r\n              <button type=\"button\" @click=\"removeEngineer(index)\" class=\"remove-engineer\">\r\n                <i class=\"fas fa-times\"></i>\r\n              </button>\r\n            </div>\r\n            <input \r\n              type=\"text\" \r\n              v-model=\"newEngineer\" \r\n              @keydown.enter.prevent=\"addEngineer\"\r\n              placeholder=\"Add engineer and press Enter\"\r\n            >\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"form-group full-width\">\r\n          <label>Documents</label>\r\n          <div class=\"file-upload\">\r\n            <input \r\n              type=\"file\" \r\n              @change=\"handleFileUpload\" \r\n              multiple\r\n              accept=\".pdf,.doc,.docx,.xls,.xlsx\"\r\n            >\r\n            <div class=\"uploaded-files\">\r\n              <div v-for=\"(file, index) in formData.documents\" :key=\"index\" class=\"file-tag\">\r\n                {{ file.name }}\r\n                <button type=\"button\" @click=\"removeFile(index)\" class=\"remove-file\">\r\n                  <i class=\"fas fa-times\"></i>\r\n                </button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"form-actions\">\r\n          <button type=\"button\" class=\"cancel-button\" @click=\"$emit('close')\">Cancel</button>\r\n          <button type=\"submit\" class=\"save-button\">Save Project</button>\r\n        </div>\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'ProjectModal',\r\n  props: {\r\n    project: {\r\n      type: Object,\r\n      default: null\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      formData: {\r\n        title: '',\r\n        department: '',\r\n        status: 'planned',\r\n        start_date: '',\r\n        end_date: '',\r\n        budget: null,\r\n        description: '',\r\n        assigned_engineers: [],\r\n        documents: []\r\n      },\r\n      newEngineer: '',\r\n      departments: [\r\n        'Water Supply',\r\n        'Roads & Bridges',\r\n        'Sanitation',\r\n        'Electricity',\r\n        'Public Works'\r\n      ]\r\n    }\r\n  },\r\n  watch: {\r\n    project: {\r\n      immediate: true,\r\n      handler(newProject) {\r\n        if (newProject) {\r\n          this.formData = { ...newProject }\r\n        }\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    handleSubmit() {\r\n      this.$emit('save', this.formData)\r\n    },\r\n    addEngineer() {\r\n      if (this.newEngineer.trim()) {\r\n        this.formData.assigned_engineers.push(this.newEngineer.trim())\r\n        this.newEngineer = ''\r\n      }\r\n    },\r\n    removeEngineer(index) {\r\n      this.formData.assigned_engineers.splice(index, 1)\r\n    },\r\n    handleFileUpload(event) {\r\n      const files = Array.from(event.target.files)\r\n      files.forEach(file => {\r\n        this.formData.documents.push({\r\n          name: file.name,\r\n          file: file\r\n        })\r\n      })\r\n    },\r\n    removeFile(index) {\r\n      this.formData.documents.splice(index, 1)\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.modal-overlay {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n  background: rgba(0, 0, 0, 0.5);\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  z-index: 1000;\r\n}\r\n\r\n.modal-content {\r\n  background: #ffffff;\r\n  border-radius: 12px;\r\n  width: 90%;\r\n  max-width: 800px;\r\n  max-height: 90vh;\r\n  overflow-y: auto;\r\n  padding: 2rem;\r\n}\r\n\r\n.modal-header {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  margin-bottom: 2rem;\r\n  padding-bottom: 1rem;\r\n  border-bottom: 2px solid rgba(37, 99, 235, 0.1);\r\n}\r\n\r